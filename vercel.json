{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/auth",
      "dest": "index.js"
    },
    {
      "src": "/products",
      "dest": "index.js"
    },
    {
      "src": "/products/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/categories",
      "dest": "index.js"
    },
    {
      "src": "/brands",
      "dest": "index.js"
    },
    {
      "src": "/",
      "dest": "dist/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "dist/$1"
    }
  ]
}
