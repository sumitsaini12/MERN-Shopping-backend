{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes" : [
      {
        "scr" : "/auth",
        "dest" : "index.js"
      },
    {
      "scr" : "/products",
      "dest" : "index.js"
    },
    {
      "scr" : "/products/(.*)",
      "dest" : "index.js"
    },
    {
      "scr" : "/",
      "dest" : "dist/index.html"
    },
    {
      "scr" : "/(.+)",
      "dest" : "dist/$1"
    }
    ]
  
  }

  
  